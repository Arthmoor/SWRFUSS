#MUDPROG
Progtype  rand_prog~
Arglist   100~
Comlist   if rand(5)
  french $r
else
  if rand(10)
    grope $r
  else
    if rand(15)
      lick $r
    else
      if rand(20)
        moan
      else
        if rand(25)
          swoon
        else
          if rand(30)
            fondle prost
          else
            if rand(35)
              pounce $r
            else
              kiss $r
            endif
          endif
        endif
      endif
    endif
  endif
endif
~
#ENDPROG

#ENDMOBILE

